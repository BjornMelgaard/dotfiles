snippet functions
  function functions(obj, withInherited = true) {
    let methods = [];
    for (let m in obj) {
      if (typeof obj[m] == "function") methods.push(m)
    }
    return withInherited ?  methods : methods.filter(m => obj.hasOwnProperty(m))
  }
