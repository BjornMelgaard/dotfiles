snippet console-log
alias cl
options head
  console.log(${0:TARGET})

snippet trace
alias tr
options head
  trace('${0:TARGET}', $0)

snippet functions
  function functions(obj, withInherited = true) {
    let methods = [];
    for (let m in obj) {
      if (typeof obj[m] == "function") methods.push(m)
    }
    return withInherited ?  methods : methods.filter(m => obj.hasOwnProperty(m))
  }
